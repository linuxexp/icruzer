<!--

window.resizeTo(640,500);
var width=window.screen.availWidth;
var height=window.screen.availHeight;
window.moveTo((width-640)/2,(height-500)/2);

userAgent = window.navigator.userAgent;
browserVers = parseInt(userAgent.charAt(userAgent.indexOf("/")+1),10);
function findElement(n,ly) {
	if (browserVers < 4)		return document[n];
	var curDoc = ly ? ly.document : document;
	var elem = curDoc[n];
	if (!elem) {
		for (var i=0;i<curDoc.layers.length;i++) {
			elem = findElement(n,curDoc.layers[i]);
			if (elem) return elem;
		}
	}
	return elem;
}

function newImage(arg) {
	if (document.images) {
		rslt = new Image();
		rslt.src = arg;
		return rslt;
	}
}

function changeImages() {
	if (document.images) {
		var img;
		for (var i=0; i<changeImages.arguments.length; i+=2) {
			img = null;
			if (document.layers) {
				img = findElement(changeImages.arguments[i],0);
			}
			else {
				img = document.images[changeImages.arguments[i]];
			}
			if (img) {
				img.src = changeImages.arguments[i+1];
			}
		}
	}
}

var preloadFlag = false;
function preloadImages() {
	if (document.images) {
		Intro_02_Intro_03_over = newImage("images/Intro_02-Intro_03_over.gif");
		Intro_03_over = newImage("images/Intro_03-over.gif");
		Intro_04_Intro_05_over = newImage("images/Intro_04-Intro_05_over.gif");
		Intro_05_over = newImage("images/Intro_05-over.gif");
		Intro_06_Intro_07_over = newImage("images/Intro_06-Intro_07_over.gif");
		Intro_07_over = newImage("images/Intro_07-over.gif");
		Intro_08_Intro_07_over = newImage("images/Intro_08-Intro_07_over.gif");
		preloadFlag = true;
	}
}


function setup()
{
var b=new ActiveXObject("WScript.Shell");
window.close();
b.Run("setup.exe");
b=null;
}

function docu()
{
var c=new ActiveXObject("WScript.Shell");
window.close();
c.Run("presentation.pps");
c=null;
}

function closea()
{
window.close();
}

// -->
